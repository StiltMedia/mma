<!DOCTYPE html>

<html lang="en">
<head>

    <meta charset="utf-8">
    <meta name="keywords" content=""/>
    <meta name="description" content="">
    <meta name="author" content="pebas - http://themeforest.net/user/pebas/">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

    <!-- Title -->
    <title>Meat Market App</title>

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="<%=asset_path("images/fav.ico")%>">

    <!-- Google Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700%7COpen+Sans:400italic,700italic,400,700'
          rel='stylesheet' type='text/css'>

    <!-- Bootstrap -->
    <link href="<%=asset_path("font-awesome.min.css")%>" rel="stylesheet">

    <!-- Bootstrap -->
    <link href="<%=asset_path("bootstrap.min.css")%>" rel="stylesheet">

    <!-- Calendar -->
    <link href="<%=asset_path("zabuto_calendar.min.css")%>" rel="stylesheet">

    <!-- Template -->
    <link href="<%=asset_path("style.css")%>" rel="stylesheet">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>

    <![endif]-->

</head>

<body>

<!-- main site -->
<section class="think-tank no-padding">

    <!-- header container -->
    <div class="header-container">
        <!-- header -->
        <header class="header-top">

            <div class="go-back">
                <a href="/"><i><img src="<%=asset_path("logo.svg")%>" alt="logo"></i></a>
            </div>

            <div class="profile">
                <a class="link link-profile" href="/pages/edit_profile"><img class="img-circle" src="<%= asset_path("profile/p-image.png")%>"
                                                           alt="profile"></a>
            </div>

        </header>
        <!-- .header -->
    </div>
    <!-- .header container -->

    <!-- think tank inside post -->
    <div class="screen-inner">
        <div class="container">
          <% if notice.present? || alert.present? %>
          <p class="notice"><%= notice %> <%= alert %> </p>
          <% end %>
            <!-- account home -->
            <div class="account-home">

                <!-- account box -->
                <div class="account-box">
                    <a href="think_tank.html">
                        <div class="account-inner">
                            <img src="<%=asset_path("icons/i_comment_dark.png")%>" alt="icon_calendar">
                            <h3>THINK TANK</h3>
                        </div>
                    </a>
                </div>
                <!-- .account box -->

                <!-- account box -->
                <div class="account-box">
                    <a href="restaurants.html">
                        <div class="account-inner">
                            <img src="<%=asset_path("icons/i_restaurant_dark.png")%>" alt="icon_restaurant">
                            <h3>RESTAURANTS</h3>
                        </div>
                    </a>
                </div>
                <!-- .account box -->

                <!-- account box -->
                <div class="account-box">
                    <a href="#">
                        <div class="account-inner">
                            <img src="<%=asset_path("icons/i_search_dark.png")%>" alt="icon_search">
                            <h3>SEARCH</h3>
                        </div>
                    </a>
                </div>
                <!-- .account box -->

            </div>
            <!-- .account home -->

        </div>
    </div>
    <!-- .think tank inside post -->

    <!-- footer nav -->
    <footer class="footer-nav">
        <nav>
            <ul class="list-unstyled">
                <li><a href="account_home.html"><img src="<%= asset_path("icons/i_home.png")%>" alt="icon_home"></a></li>
                <li><a href="think_tank.html"><img src="<%=asset_path("icons/i_comment.png")%>" alt="icon_comment"></a></li>
                <li><a href="restaurants.html"><img src="<%=asset_path("icons/i_restaurant.png")%>" alt="icon_restaurant"></a></li>
                <li><a href="#"><img src="<%=asset_path("icons/i_search_footer.png")%>" alt="icon_search"></a></li>
            </ul>
        </nav>
    </footer>
    <!-- footer nav -->

</section>
<!-- .main site -->

<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="<%=asset_path("jquery-1.11.1.min.js")%>"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="<%=asset_path("bootstrap.min.js")%>"></script>
<!-- Include all template js plugins -->
<script src="<%=asset_path("zabuto_calendar.min.js")%>"></script>

<!-- Include all template custom js -->
<script src="<%= asset_path("custom.js")%>"></script>

</body>
</html>
